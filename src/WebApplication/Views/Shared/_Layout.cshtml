<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />

    <meta http-equiv="x-ua-compatible" content="IE=edge" />
    <meta http-equiv="cleartype" content="on">
    <meta http-equiv="Content-Language" content="pt-br">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">

    <meta name="MobileOptimized" content="320">
    <meta name="HandheldFriendly" content="True">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="description" content="@ViewBag.MetaDescription" />
    <meta name="rating" content="Geral" />
    <meta name="language" content="portuguese" />
    <meta name="distribution" content="Global" />
    <meta name="keywords" content="escola,colégio,berçário,zona leste,tatuapé,anália franco,ensino fundamental,educação infantil" />
    <meta name="author" content="Eco Aprender | Escola de Educação Infantil e Berçário" />
    <meta name="publisher" content="Eco Aprender | Escola de Educação Infantil e Berçário" />

    <meta name="google-site-verification" content="2eLPaqpbOulcjZzSK-UUmm-_H0IKKCnw-xjdS9AQZqQ" />
    <meta name="p:domain_verify" content="b50ccdada695de10337a74d0ad85aa9a" />

    <link href='https://plus.google.com/+EcoaprenderBr/posts' rel='publisher' />

    <link href='//fonts.googleapis.com/css?family=Fugaz+One|Raleway:400,100,200,300,500,600,700,800,900' rel='stylesheet' type='text/css'>
    <link rel='stylesheet prefetch' href='//netdna.bootstrapcdn.com/font-awesome/4.0.0/css/font-awesome.min.css'>
    <link rel='stylesheet' href='//fonts.googleapis.com/css?family=Montserrat' type='text/css'>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link rel='stylesheet' href='//cdn.datatables.net/1.10.10/css/jquery.dataTables.min.css' type='text/css'>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <link rel="shortcut icon" type="image/png" href="/img/escola-de-educacao-infantil-e-bercario-eco-aprender.gif" />

    @RenderSection("Head", false)
    @RenderSection("css", false)

    <title>@ViewBag.Title</title>

    @Styles.Render("~/css")
</head>
<body>
    <!-- Modal -->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">Rematrículas Eco Aprender</h4>
                </div>
                <div class="modal-body">
                    <img src="~/img/home/rematricula.jpg" alt="Rematrículas Eco Aprender" title="Rematrículas Eco Aprender" class="img-responsive" />
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <nav id="menu-site" class="hide menu-mobile menu-site slideout-menu">
        <header>
            <h2>ESCOLA ECO APRENDER</h2>
        </header>
        <ul>
            <li><a href="/Sobre">Sobre a Escola</a></li>
            <li><a href="/programa-bilingue">Programa Bilíngue</a></li>
            <li><a href="/instagram" title="Instagram">Instagram</a></li>
            <li><a href="/facebook" title="Facebook">Facebook</a></li>
            <li><a href="/blog-por-categoria/todas" title="Blog">Blog</a></li>
            <li><a href="/videos" title="Videos">Videos</a></li>
            <li><a href="/fotos" title="Fotos">Fotos</a></li>
            <li><a href="/Professor">Colaboradores</a></li>
            <li><a href="/portal-do-aluno">Portal do Aluno</a></li>
            <li><a href="/fale-conosco">Fale com a Escola</a></li>
        </ul>
    </nav>

    @{
        var usuarioNome = (ViewBag.UsuarioNome != null ? ViewBag.UsuarioNome : "");
        var usuarioFoto = (ViewBag.UsuarioFoto != null ? ViewBag.UsuarioFoto : "");
    }

    <nav id="menu-admin" class="hide menu-mobile menu-admin">
        <header>
            <h2>@usuarioNome</h2>
        </header>
        <ul>
            <li><a href="@Url.Action("Painel", "Portal-Do-Aluno")">Painel</a></li>
            @if (Session["UsuarioPerfilId"] != null && Convert.ToInt32(Session["UsuarioPerfilId"]) == 1)
            {
                <li>
                    <a href="#" title="Admin">Admin</a>
                    <div class="sub-item"><a href="~/Portal-Do-Aluno/Usuario" title="Usuários">Usuários</a></div>
                    <div class="sub-item"><a href="~/portal-do-aluno/admin/comunicado" title="Comunicado">Comunicado</a></div>
                    <div class="sub-item"><a href="~/portal-do-aluno/admin/atividade" title="Atividade">Atividades</a></div>
                    <div class="sub-item"><a href="~/portal-do-aluno/admin/video" title="Videos">Videos</a></div>
                </li>
            }
            <li><a href="@Url.Action("Sair", "Portal-Do-Aluno")">Sair</a></li>
        </ul>
    </nav>

    <main id="panel" class="scrollable">
        <section id="menu-site">
            <nav class="navbar navbar-default navbar-ecoaprender">
                <div class="container-fluid paddingRight0">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed float-left menu menu-bar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/">Escola Eco Aprender</a>
                    </div>

                    <div class="collapse navbar-collapse float-right menu-links paddingRight0">
                        <ul class="nav navbar-nav menu-site hide">
                            <li class="liSobre"><a href="/Sobre">Sobre a Escola</a></li>
                            <li class="liProgramaBilingue"><a href="/programa-bilingue">Programa Bilíngue</a></li>
                            <li class="liAtividades"><a href="/instagram" title="Instagram">Instagram</a></li>
                            <li class="liFacebook"><a href="/facebook" title="Facebook">Facebook</a></li>
                            <li class="liBlog"><a href="/blog-por-categoria/todas" title="Blog">Blog</a></li>
                            <li class="liVideos"><a href="/videos" title="Videos">Videos</a></li>
                            <li class="liFotos"><a href="/fotos" title="Fotos">Fotos</a></li>
                            <li class="liProfessor"><a href="/Professor">Colaboradores</a></li>
                            <li class="liPortaldoAluno"><a href="/portal-do-aluno">Portal do Aluno</a></li>
                            <li class="liFaleConosco"><a href="/fale-conosco">Fale com a Escola</a></li>
                        </ul>

                        <ul class="nav navbar-nav menu-admin hide">
                            <li><a href="@Url.Action("Painel", "Portal-Do-Aluno")">Painel</a></li>
                            @if (Session["UsuarioPerfilId"] != null && Convert.ToInt32(Session["UsuarioPerfilId"]) == 1)
                            {
                                <li class="dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Admin</a>
                                    <ul class="dropdown-menu">
                                        <li><a href="@Url.Action("Usuario", "Portal-Do-Aluno")">Usuário</a></li>
                                        <li><a href="@Url.Action("Index", "portal-do-aluno/admin/comunicado")">Comunicado</a></li>
                                        <li><a href="@Url.Action("atividade", "portal-do-aluno/admin")">Atividades</a></li>
                                        <li><a href="@Url.Action("Index", "portal-do-aluno/admin/video")">Videos</a></li>
                                    </ul>
                                </li>
                            }
                            <li><a href="@Url.Action("Sair", "Portal-Do-Aluno")">Sair</a></li>
                        </ul>
                    </div>

                </div>
            </nav>
        </section>

        <section id="main" class="render-body">
            @RenderBody()
        </section>

        <footer>
            <section class="redes-sociais">
                <div class="container">
                    <div class="row">
                        <h2 class="col-xs-12 redesSociaisTitulo">ECO APRENDER NAS REDES SOCIAIS</h2>
                        <ul>
                            <li>
                                <a href="https://www.facebook.com/EscolaEcoAprender" target="_blank" class="float-right">
                                    <span class="icone facebook"></span>
                                    <p>facebook</p>
                                </a>
                            </li>
                            <li>
                                <a href="https://instagram.com/ecoaprender/" target="_blank" class="float-left">
                                    <span class="icone instagram"></span>
                                    <p>instagram</p>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </section>

            <section id="mapa-do-site" class="div-mapa-do-site">
                <div class="container">
                    <div class="row">
                        <ul class="col-xs-6 col-md-2">
                            <li class="titulo"><h2><strong>INSTITUCIONAL</strong></h2></li>
                            <li><a href='/' title="Escola Eco Aprender">Home</a></li>
                            <li><a href="/sobre" title="Sobre a Escola">Sobre a Escola</a></li>
                            <li><a href="/programa-bilingue" title="Bilíngue">Programa Bilíngue</a></li>
                            <li><a href="/professor" title="Quadro de Professores">Colaboradores</a></li>
                        </ul>
                        <ul class="col-xs-6 col-md-2">
                            <li class="titulo"><h2><strong>NA ESCOLA</strong></h2></li>
                            <li><a href='/blog-por-categoria/todas' title="Blog">Blog</a></li>
                            <li><a href='/videos' title="Videos">Videos</a></li>
                            <li></li>
                        </ul>
                        <ul class="col-xs-6 col-md-2 clearBoth">
                            <li class="titulo"><h2><strong>ADMIN</strong></h2></li>
                            <li><a href='~/portal-do-aluno'>Portal do Aluno</a></li>
                        </ul>
                        <ul class="col-xs-6 col-md-2">
                            <li class="titulo"><h2><strong>ATENDIMENTO</strong></h2></li>
                            <li><a href='/fale-conosco'>Fale com a Escola</a></li>
                            <li><a href='/fale-conosco'>Como Chegar</a></li>
                            <li><a href='/fale-conosco'>Tel. 2157-2934 / 2676-2052</a></li>
                        </ul>
                        <ul class="col-xs-6 col-md-2 clearBoth">
                            <li class="titulo"><h2><strong>REDES SOCIAIS</strong></h2></li>
                            <li><a href='/facebook' target="_blank">Facebook</a></li>
                            <li><a href='/instagram' target="_blank">Instagram</a></li>
                        </ul>
                        <ul class="col-xs-6 col-md-2">
                            <li class="titulo"><h2><strong>SITES INTERESSANTES</strong></h2></li>
                            <li><a href='http://www.portaldacrianca.com.pt/' rel="nofollow">Portal da Criança</a></li>
                        </ul>
                    </div>
                </div>
            </section>

            <section id="footer-logo" class="footer" itemscope itemtype="http://schema.org/Organization">
                <div class="container">
                    <figure>
                        <img src="~/img/logo-eco-aprender.jpg" alt="Eco Aprender" class="img-responsive" itemprop="logo" />
                    </figure>
                    <h1 itemprop="name">Escola de Educação Infantil e Berçário Eco Aprender</h1>
                </div>
                <address itemprop="address">
                    <span>Rua Euclides Pacheco, 1347 - Tatuapé</span> -
                    <span>São Paulo</span>/
                    <span>SP</span>
                </address>
                <p itemprop="telephone">
                    Tel. 2157-2934 / 2676-2052
                </p>
                <p>
                    <a itemprop="url" title="Escola Eco Aprender" href="https://www.ecoaprender.com.br">www.ecoaprender.com.br</a>
                </p>
            </section>
        </footer>

        @if (!Request.Browser.IsMobileDevice)
        {
            <div class="div-redes-sociais-follow">
                @Html.Partial("_RedesSociaisFollow")
            </div>
        }
    </main>

